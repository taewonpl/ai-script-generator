{
  "name": "ai-script-generator-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@8.15.6",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "typecheck": "tsc --noEmit",
    "type:watch": "tsc --noEmit --watch",
    "prepare": "husky",
    "msw:init": "msw init public --save",
    "generate:core-types": "openapi-typescript http://localhost:8000/openapi.json -o src/shared/api/generated/types/core-types.ts",
    "generate:project-types": "openapi-typescript http://localhost:8001/openapi.json -o src/shared/api/generated/types/project-types.ts",
    "generate:generation-types": "openapi-typescript http://localhost:8002/openapi.json -o src/shared/api/generated/types/generation-types.ts",
    "generate:core-schemas": "node scripts/generate-zod-schemas.js core http://localhost:8000/openapi.json",
    "generate:project-schemas": "node scripts/generate-zod-schemas.js project http://localhost:8001/openapi.json",
    "generate:generation-schemas": "node scripts/generate-zod-schemas.js generation http://localhost:8002/openapi.json",
    "generate:all-types": "npm run generate:core-types && npm run generate:project-types && npm run generate:generation-types",
    "generate:all-schemas": "npm run generate:core-schemas && npm run generate:project-schemas && npm run generate:generation-schemas",
    "generate:all": "npm run generate:all-types && npm run generate:all-schemas",
    "dev:with-types": "npm run generate:all && npm run dev"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@sentry/react": "^10.5.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "axios": "^1.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.1",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "web-vitals": "^5.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "msw": "^2.10.5",
    "openapi-typescript": "^7.9.1",
    "openapi-zod-client": "^1.18.3",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "tsc --noEmit"
    ]
  }
}
